{
  "kind": "collectionType",
  "collectionName": "activity_sessions",
  "info": {
    "singularName": "activity-session",
    "pluralName": "activity-sessions",
    "displayName": "Activity Session"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "sessionId": {
      "type": "uid"
    },
    "activity": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::activity.activity"
    },
    "activitySessionStatus": {
      "type": "enumeration",
      "default": "pending",
      "enum": [
        "pending",
        "in_progress",
        "completed",
        "cancelled",
        "interrupted",
        "abandoned",
        "reschedule_requested"
      ]
    },
    "startAt": {
      "type": "datetime"
    },
    "endAt": {
      "type": "datetime"
    },
    "durationMinutes": {
      "type": "integer"
    },
    "estimatedScore": {
      "type": "integer"
    },
    "actualScore": {
      "type": "integer"
    },
    "promptLevel": {
      "type": "enumeration",
      "enum": [
        "independent",
        "verbal",
        "gestural",
        "partial_physical",
        "full_physical"
      ]
    },
    "rawTelemetry": {
      "type": "json"
    },
    "avgLatency": {
      "type": "float"
    },
    "successRate": {
      "type": "float"
    },
    "teacherNotes": {
      "type": "text"
    },
    "therapist": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "student": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "aiRecommendation": {
      "type": "string"
    },
    "recommendationActivity": {
      "type": "string"
    }
  }
}
