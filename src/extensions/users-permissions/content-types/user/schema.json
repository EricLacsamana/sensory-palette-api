{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": true,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": true,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": true
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": true,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": true,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": true,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": true
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": true
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": true
    },
    "firstName": {
      "type": "string",
      "configurable": true
    },
    "middleName": {
      "type": "string",
      "configurable": true
    },
    "lastName": {
      "type": "string",
      "configurable": true
    },
    "profilePicture": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "configurable": true
    },
    "dateOfBirth": {
      "type": "date",
      "configurable": true
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female"
      ],
      "configurable": true
    }
  }
}